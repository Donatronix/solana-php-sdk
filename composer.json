{
    "name": "ultainfinity/solana-php-sdk",
    "description": "Solana PHP SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ihor Porokhnenko",
            "email": "ihor.porokhnenko@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-sodium": "*",
        "illuminate/http": "~8 || ~9",
        "illuminate/support": "~8 || ~9",
        "paragonie/sodium_compat": "~1",
        "stephenhill/base58": "~1"
    },
    "require-dev": {
        "mockery/mockery": "~1",
        "orchestra/testbench": "~6",
        "phpunit/phpunit": "~9",
        "squizlabs/php_codesniffer": "~3",
        "tightenco/tlint": "~5"
    },
    "autoload": {
        "psr-4": {
            "Ultainfinity\\SolanaPhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ultainfinity\\SolanaPhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
